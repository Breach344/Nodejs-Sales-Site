//Toggle #blackout visibility
function blackout() {
  if(document.getElementById('container-wrap').style.visibility != 'visible') {
  document.getElementById('container-wrap').style.visibility = 'visible';
} else {
  document.getElementById('container-wrap').style.visibility = 'hidden';
}
  listTotal();
}
//End of toggle #blackout visibility



//Calculate total cart price
function listTotal() {
const listItems = [].slice.call(document.querySelectorAll('#the-ul li div'))

const sum = listItems.reduce(function(sum, el) {
	return sum + parseFloat(el.innerText)
}, 0);
  
  console.log(sum);

document.getElementById('list-total').textContent = 'Total: $' + (Math.round(sum * 100) / 100);
}
//End of calculate total cart price



