//Stop page scrolling
function noScroll() {
  const theBody = document.body;
 if(document.querySelector('#blackout').style.visibility == 'hidden') {
   

   theBody.style.overflow = "auto";
   theBody.style.overflowX ="hidden";
   
} else {
   if(document.querySelector('#blackout').style.visibility == 'visible') {

     theBody.style.overflow = "hidden";
}
}
}
//End of stop page scrolling



