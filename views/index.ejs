//Change appearance of ham button
function hamStyle() {
   let x = document.getElementById("ham-list-wrap");
  if (x.style.visibility === "visible") {
    x.style.visibility = "hidden";
    
document.getElementById('blackout').style.visibility ="hidden";   
    
    document.querySelector('#top-line').style.cssText = 
  'transform: rotate(0deg);top: 0.1em;position: absolute;';

   document.querySelector('#bottom-line').style.cssText = 
  'top: 1.4em';
   document.querySelector('#middle-line').style.cssText = 
  'visibility: visible;';
   document.querySelector('#button').style.cssText = 'top: 1.5em;';
    
  } else {
    x.style.visibility = "visible";
   document.getElementById('blackout').style.visibility ="visible";  
    
    document.querySelector('#top-line').style.cssText = 
  'transform: rotate(45deg);top: 2.4em;left: 19em;position: fixed;';
    
   document.querySelector('#bottom-line').style.cssText = 
  'transform: rotate(-45deg);top: 2.4em;left: 19em;position: fixed;';
    
   document.querySelector('#middle-line').style.cssText = 
  'visibility: hidden;';
     document.querySelector('#button').style.cssText = 
  'position: fixed;left: 19em;';
  }
}
//End of change appearance of ham button



//Change auth to click ham button
function btnAuth() {
    let x = document.querySelector('#blackout');
  if(x.style.visibility !== 'visible') {
    x.style.visibility = 'visible';
     document.getElementById('button').style.pointerEvents = 'none';
     document.getElementById('button').style.opacity = '20%';
    
  } else {
      x.style.visibility = 'hidden';
      document.getElementById('button').style.pointerEvents = 'auto';
     document.getElementById('button').style.opacity = '100%'; 
  }
}
//End of change auth to click ham button



